{
  "name": "application",
  "version": "0.0.1",
  "description": "idempotency",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check",
    "lint:fix": "npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run rebuild",
    "posttest": "npm run lint",
    "docker:build": "docker build -t application .",
    "docker:run": "docker run -p 3000:3000 -d application",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Lena L. <kisuni@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^6.1.3",
    "@loopback/core": "^5.1.3",
    "@loopback/logging": "^0.11.5",
    "@loopback/repository": "^6.1.4",
    "@loopback/rest": "^13.1.3",
    "@loopback/rest-explorer": "^6.1.3",
    "loopback-connector-openapi": "^6.2.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.3",
    "@loopback/testlab": "^6.1.3",
    "@types/node": "^16.18.50",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  }
}
